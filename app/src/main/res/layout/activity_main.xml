<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_on_primary_emphasis_high_type"
    tools:context=".MainActivity">

    <soup.neumorphism.NeumorphImageButton

        android:id="@+id/testbutton2"
        style="@style/Widget.Neumorph.ImageButton"
        android:layout_width="150dp"
        android:layout_height="150dp"


        android:scaleType="centerInside"
        app:layout_constraintStart_toEndOf="@+id/testbutton3"
        app:layout_constraintTop_toBottomOf="@+id/testcheckbox3"
        app:neumorph_shadowColorLight="@color/white"
        app:neumorph_shapeType="flat"
        tools:ignore="MissingConstraints">


    </soup.neumorphism.NeumorphImageButton>

    <soup.neumorphism.NeumorphImageButton

        android:id="@+id/testbutton3"
        style="@style/Widget.Neumorph.ImageButton"
        android:layout_width="150dp"
        android:layout_height="150dp"


        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testbutton"
        app:neumorph_shadowColorLight="@color/white"
        app:neumorph_shapeType="flat"
        tools:ignore="MissingConstraints">


    </soup.neumorphism.NeumorphImageButton>

    <soup.neumorphism.NeumorphCardView

        android:id="@+id/progressbarcontainer"
        style="@style/Widget.Neumorph.CardView"


        android:layout_width="59dp"
        android:layout_height="match_parent"
        android:layout_marginVertical="25dp"
        android:layout_marginRight="7pt"
        android:background="@drawable/violet"
        app:layout_constraintEnd_toEndOf="parent"
        app:neumorph_shadowElevation="3dp"
        app:neumorph_shapeType="basin"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="6dp">

        <soup.neumorphism.NeumorphCardView

            android:id="@+id/progressbar"
            style="@style/Widget.Neumorph.CardView"
            android:layout_width="0.22in"
            android:layout_height="match_parent"
            android:layout_marginVertical="10dp"
            android:layout_marginTop="48dp"


            app:layout_constraintEnd_toEndOf="parent"
            app:neumorph_shadowColorDark="@color/black"
            app:neumorph_shadowColorLight="@color/white"
            app:neumorph_shadowElevation="3dp"
            app:neumorph_shapeType="pressed" />
    </soup.neumorphism.NeumorphCardView>


    <soup.neumorphism.NeumorphImageButton

        android:id="@+id/testbutton"
        style="@style/Widget.Neumorph.ImageButton"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/terminal"
        app:neumorph_shadowColorLight="@color/white"
        app:neumorph_shapeType="flat"
        app:neumorph_strokeWidth="60dp"
        tools:ignore="MissingConstraints">


    </soup.neumorphism.NeumorphImageButton>


    <soup.neumorphism.NeumorphCardView

        android:id="@+id/terminal"
        style="@style/CardView"

        android:layout_width="329dp"
        android:layout_height="132dp"
        android:layout_marginVertical="25dp"

        app:neumorph_backgroundColor="@color/black"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toTopOf="parent"
        app:neumorph_shadowElevation="4dp"
        app:neumorph_shapeType="pressed"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="249dp"
            android:layout_height="54dp"
            android:text="TextView"

            android:textColor="@color/design_default_color_secondary_variant" />
    </soup.neumorphism.NeumorphCardView>

    <soup.neumorphism.NeumorphCardView

        android:id="@+id/testcheckbox3"
        style="@style/CardView"

        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginVertical="25dp"


        app:layout_constraintStart_toEndOf="@+id/testbutton"
        app:layout_constraintTop_toBottomOf="@+id/terminal"
        app:neumorph_shadowElevation="3dp"
        app:neumorph_shapeType="basin"
        tools:ignore="MissingConstraints">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="127dp"
            android:layout_height="126dp">


            <soup.neumorphism.NeumorphCardView

                android:id="@+id/testcheckbox"
                style="@style/CardView"
                android:layout_width="70dp"
                android:layout_height="70dp"


                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="pressed"
                tools:ignore="MissingConstraints">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RadioButton
                        android:id="@+id/radio1"
                        android:layout_width="23dp"
                        android:layout_height="31dp"
                        android:button="@drawable/ic_right"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.533"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.538" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </soup.neumorphism.NeumorphCardView>

            <soup.neumorphism.NeumorphCardView

                android:id="@+id/testcheckbox1"
                style="@style/CardView"

                android:layout_width="70dp"
                android:layout_height="70dp"


                android:layout_marginBottom="4dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:neumorph_shadowElevation="3dp"
                app:neumorph_shapeType="pressed"
                tools:ignore="MissingConstraints">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RadioButton
                        android:id="@+id/radio2"
                        android:layout_width="23dp"
                        android:layout_height="30dp"
                        android:button="@drawable/ic_left"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </soup.neumorphism.NeumorphCardView>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </soup.neumorphism.NeumorphCardView>


</androidx.constraintlayout.widget.ConstraintLayout>